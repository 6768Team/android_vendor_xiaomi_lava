{
    "version":"1.0",
    "product":"merlin",
    "testitems":[
        {
            "name":"home_version_info_test",
            "caption":"版本信息测试"

        },
        {
            "name":"home_sim_card_check_test",
            "caption":"SIM卡测试"

        },
        {
            "name":"home_keyboard_check_test",
            "caption":"按键测试",
            "config":{
                "support_check_keys": [
                    "volumeUp_down",
                    "volumeUp_up",
                    "volumeDown_down",
                    "volumeDown_up", 
                    "power_down",
                    "power_up"
                ]
            }
        },
        {
            "name":"home_vibrator_check_test",
            "caption":"震动测试"

        },
        {
            "name": "home_led_test",
            "caption": "LED灯测试",
            "config": {
                "support_checked_leds": [
                    {
                        "name": "cit_lcdlight_brightness",
                        "type": "screen_brightness",
                        "max_brightness":2047,
                        "breath":true
                    },
                    {
                        "name": "cit_torch_first",
                        "type": "general_camera_flash",
                        "torch_node_path": "/sys/class/leds/torch-light0/brightness",
                        "max_brightness": 255,
                        "min_brightness": 0
                    }
                ]
            }
        },
        {
            "name":"home_touch_panel_test",
            "caption":"触摸屏测试"

